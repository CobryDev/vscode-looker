{
    "name": "looker",
    "displayName": "looker",
    "description": "Tools to aid development of LookML",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:lookml"
    ],
    "contributes": {
        "commands": [
            {
                "command": "looker.savePassword",
                "title": "Save Looker API Secret"
            },
            {
                "command": "looker.apiLogin",
                "title": "Log in to Looker API."
            }
        ],
        "languages": [
            {
                "id": "lookml",
                "aliases": [
                    "LookML",
                    "lookml"
                ],
                "extensions": [
                    ".lkml"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "lookml",
                "scopeName": "source.lkml",
                "path": "./syntaxes/lookml.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.block.sql": "source.sql"
                }
            }
        ],
        "snippets": [
            {
                "language": "lookml",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "npm run compile && node ./node_modules/@vscode/test-electron/out/runTest.js"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "@types/vscode": "^1.32.0",
        "@vscode/test-electron": "^2.3.8",
        "tslint": "^5.12.1",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/glob": "^8.0.0",
        "glob": "^10.3.10",
        "keytar": "^7.9.0"
    }
}