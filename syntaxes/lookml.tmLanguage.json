{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "LookML",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#sql"
		},
		{
			"include": "#comment-line"
		},
		{
			"include": "#types"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.lookml",
				"match": "\\b(view|sql_table_name|suggestions|extends|extension|required_access_grants|derived_table|explore_source|bind_filters|from_field|to_field|column|field|derived_column|sql|expressions_custom_filter|filters|value|limit|sort|desc|timezone|persist_for|sql_trigger_value|cluster_keys|datagroup_trigger|distribution|distribution_style|sortkeys|indexes|partition_keys|create_process|sql_step|sql_create|SQLquery|set|fields|dimension|dimension_group|measure|filter|parameter|label|label_from_parameter|view_label|group_label|group_item_label|description|hidden|alias|value_format|value_format_name|html|required_fields|drill_fields|can_filter|fanout_on|tags|type|primary_key|case|when|alpha_sort|tiers|style|sql_latitude|sql_longitude|suggestable|suggest_persist_for|suggest_dimension|suggest_explore|allowed_value|bypass_suggest_restrictions|full_suggestions|skip_drill_filter|case_sensitive|order_by_field|map_layer_name|link|url|icon_url|timeframes|convert_tz|datatype|intervals|sql_start|sql_end|direction|approximate|approximate_threshold|sql_distinct_key|list_field|percentile|precision|default_value|connection|includefiscal_month_offset|persist_with|week_start_day|named_value_format|value_formatmap_layer|extents_json_url|feature_key|file|format|max_zoom_level|min_zoom_level|projection|property_key|property_label_key|urldatagroup|sql_trigger|max_cache_age|access_grant|user_attribute|allowed_values|explore|symmetric_aggregates|fieldssql_always_where|required_access_grantsalways_filter|conditionally_filter|valueunlessaccess_filter|user_attributealways_joinjoin|relationship|from|required_joins|foreign_key|sql_on|required_access_grantspersist_for|view_name|cancel_grouping_fields)\\b"
			}]
		},
		"variables": {
			"name": "support.variable",
			"begin": "\\${",
			"end": "}",
			"patterns": [{
				"name": "constant.character.escape.lookml",
				"match": "\\\\."
			}]
		},
		"types" : {
			"name": "support.type",
			"match": "\\b(yesno|yes|no|epoch|timestamp|datetime|date|yyyymmdd|feet|kilometers|meters|miles|nautical_miles|yards|classic|interval|integer|relational)\\b",
			"patterns": [{}]
		},
		"comment-line": {
			"name": "comment.line.block",
			"begin": "(//|#)",
			"end": "\n",
			"patterns": [{}]
		},
		"strings": {
			"name": "string.quoted.double.lookml",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.lookml",
					"match": "\\\\."
				},
				{
					"name": "constant.character.escape.lookml",
					"match": "#."
				}
			]
		},
		"sql": {
			"name": "sql.lookml",
			"begin": "sql:",
			"end": ";;",
			"patterns": [
				{
					"name": "constant.character.escape.lookml",
					"match": "\\\\."
				},
				{
					"name": "constant.character.escape.lookml",
					"match": "#."
				}
			]
		}
	},
	"scopeName": "source.lkml"
}