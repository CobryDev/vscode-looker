{
	"Dimension": {
		"prefix": "dim",
		"body": [
            "dimension: ${1:name} {",
            "\t\tlabel: \"${2:Readable Dimension Name}\"",
            "\t\tdescription: \"${3:Description of the dimension}\"",
            "\t\ttype: ${4:type}",
            "\t\tsql: ${5:SQL string} ;;",
            "}"
		],
		"description": "Create Looker dimension."
    },
    "Measure": {
		"prefix": "mes",
		"body": [
            "measure: ${1:name} {",
            "\t\tlabel: \"${2:Readable Dimension Name}\"",
            "\t\tdescription: \"${3:Description of the dimension}\"",
            "\t\ttype: ${4:type}",
            "\t\tsql: ${5:SQL string} ;;",
            "}"
		],
		"description": "Create Looker measure."
    },
    "Filter": {
		"prefix": "fil",
		"body": [
            "filter: ${1:name} {",
            "\t\tlabel: \"${2:Readable Filter Name}\"",
            "\t\tdescription: \"${3:Description of the filter}\"",
            "\t\ttype: ${4:string}",
            "\t\tsuggest_dimension: ${5:id}",
            "\t\tsuggest_persist_for: \"${6:24 hours}\"",
            "}"
		],
		"description": "Create Looker dimension."
	}
    
}